{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ 生成活动文件",
            "command": "/usr/bin/c++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                // "/usr/local/lib64/libfmt.a",
                "-I/usr/local/include",
                "-L/usr/local/lib",
                "-L/usr/local/lib64",
                "-I${workspaceFolder}/contrib/include",
                "-I${workspaceFolder}/src/include",
                "-lpthread",
                "-lworkflow",
                // "-llmdb",
                "-std=c++11",
                "${file}",
                "${workspaceFolder}/src/third_party/ipcity/ipcityReader.cpp",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}.elf"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        },
    ],
    "version": "2.0.0"
}
